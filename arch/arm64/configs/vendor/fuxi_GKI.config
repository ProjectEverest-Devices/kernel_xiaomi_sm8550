# Arch
# CONFIG_ARCH_CROW is not set

# Camera
CONFIG_MI_CAMERA_FUXI=y

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=m
CONFIG_INPUT_FINGERPRINT=y

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# MTD
CONFIG_MTD=m
CONFIG_MTD_BLOCK=m
CONFIG_MTD_BLOCK2MTD=m
CONFIG_MTD_OOPS=m

# Power supply
CONFIG_MI_WIRELESS=y

# Product
CONFIG_TARGET_PRODUCT_FUXI=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# Xiaomi HWID
CONFIG_MI_HARDWARE_ID=m

# ZRAM
CONFIG_ZRAM_WRITEBACK=y

# CONFIG_QCOM_WCD_USBSS_I2C is not set
